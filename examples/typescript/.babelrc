{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/typescript"
  ],
  "compact": true,
  "plugins": [
    [
      "transform-import-to-read-file-sync",
      {
        "test": "\\.graphql$",
        "options": "utf8"
      }
    ],
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": [
          "Error"
        ]
      }
    ],
    "@babel/proposal-class-properties"
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ]
    }
  }
}