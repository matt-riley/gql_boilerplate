{
  "name": "gql_boilerplate",
  "version": "2.0.2",
  "repository": "git@github.com:matt-riley/gql_boilerplate.git",
  "author": "Matt Riley <matt@mattriley.me> (https://mattriley.me)",
  "license": "MIT",
  "options": {
    "tests": "test/"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src -d build --copy-files --no-comments && npm run update-schema",
    "start": "nodemon --watch src -e js,graphql --exec babel-node -- src/server/index.js",
    "update-schema": "NODE_ENV=local babel-node ./scripts/build_schema.js",
    "test": "npm run lint && npm run test-only",
    "test-only": "NODE_ENV=test nyc --reporter=lcov mocha test/setup.js $npm_package_options_tests --timeout 2000",
    "lint": "eslint src test",
    "lint-fix": "eslint \"src/**/*.js\" \"test/**/*.js\" --fix"
  },
  "dependencies": {
    "apollo-datasource-rest": "0.2.0",
    "apollo-server-express": "2.2.0",
    "express": "4.16.4",
    "graphql": "14.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-import-to-read-file-sync": "0.1.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-minify": "0.5.0",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.5.0",
    "chai-subset": "1.6.0",
    "eslint": "5.8.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-mocha": "5.2.0",
    "mocha": "5.2.0",
    "mocha-sinon": "2.1.0",
    "nodemon": "1.18.6",
    "nyc": "13.1.0",
    "sinon": "6.3.5",
    "sinon-chai": "3.2.0"
  }
}
